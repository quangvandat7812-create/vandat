local CurrentTab = "Main"

local function CreateTabButton(name, position)
    local btn = Instance.new("TextButton")
    btn.Text = name
    btn.Size = UDim2.new(1, 0, 0, 40)
    btn.Position = UDim2.new(0, 0, 0, position * 40)
    btn.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.Parent = TabsFrame
    
    btn.MouseButton1Click:Connect(function()
        CurrentTab = name
        UpdateContent()
    end)
end

for i, tab in pairs(Tabs) do
    CreateTabButton(tab, i-1)
end

-- Content
local function CreateToggle(name, callback)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -20, 0, 30)
    frame.BackgroundTransparency = 1
    frame.Parent = ContentFrame
    
    local toggle = Instance.new("TextButton")
    toggle.Text = name
    toggle.Size = UDim2.new(1, 0, 1, 0)
    toggle.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
    toggle.Parent = frame
    
    local state = false
    
    toggle.MouseButton1Click:Connect(function()
        state = not state
        if state then
            toggle.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
        else
            toggle.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        end
        callback(state)
    end)
    
    return frame
end

local ContentElements = {}

function UpdateContent()
    for _, child in pairs(ContentFrame:GetChildren()) do
        child:Destroy()
    end
    
    if CurrentTab == "Main" then
        table.insert(ContentElements, CreateToggle("Auto Farm Level", function(state)
            Settings.AutoFarm = state
            if state then AutoFarmLevel() end
        end))
        
        table.insert(ContentElements, CreateToggle("Auto New World", function(state)
            Settings.AutoNewWorld = state
            if state then AutoNewWorld() end
        end))
        
        table.insert(ContentElements, CreateToggle("Auto Third Sea", function(state)
            Settings.AutoThirdSea = state
            if state then AutoThirdSea() end
        end))
        
        for i, element in pairs(ContentElements) do
            element.Position = UDim2.new(0, 10, 0, (i-1)*35 + 10)
        end
        
    elseif CurrentTab == "Raid" then
        table.insert(ContentElements, CreateToggle("Auto Raid", function(state)
            Settings.AutoRaid = state
            if state then AutoRaid() end
        end))
        
        table.insert(ContentElements, CreateToggle("Auto Bartilo", function(state)
            Settings.AutoBartilo = state
            if state then AutoBartilo() end
        end))
        
        for i, element in pairs(ContentElements) do
            element.Position = UDim2.new(0, 10, 0, (i-1)*35 + 10)
        end
        
    elseif CurrentTab == "Boss" then
        table.insert(ContentElements, CreateToggle("Auto Elite", function(state)
            Settings.AutoElite = state
            if state then AutoElite() end
        end))
        
        table.insert(ContentElements, CreateToggle("Auto Saber", function(state)
            Settings.AutoSaber = state
            if state then AutoSaber() end
        end))
        
        for i, element in pairs(ContentElements) do
            element.Position = UDim2.new(0, 10, 0, (i-1)*35 + 10)
        end
        
    elseif CurrentTab == "Player" then
        -- Player name input
        local nameFrame = Instance.new("Frame")
        nameFrame.Size = UDim2.new(1, -20, 0, 30)
        nameFrame.BackgroundTransparency = 1
        nameFrame.Position = UDim2.new(0, 10, 0, 10)
        nameFrame.Parent = ContentFrame
        
        local nameBox = Instance.new("TextBox")
        nameBox.Size = UDim2.new(1, 0, 1, 0)
        nameBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        nameBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameBox.PlaceholderText = "Player Name"
        nameBox.Parent = nameFrame
        
        nameBox.FocusLost:Connect(function()
            Settings.TargetPlayer = nameBox.Text
        end)
        
        table.insert(ContentElements, CreateToggle("Teleport To Player", function(state)
            Settings.TeleportToPlayer = state
            if state then TeleportToPlayer() end
        end))
        
        for i, element in pairs(ContentElements) do
            element.Position = UDim2.new(0, 10, 0, (i-1)*35 + 50)
        end
    end
end

-- Functions
function AutoFarmLevel()
    spawn(function()
        while Settings.AutoFarm do
            pcall(function()
                for i,v in pairs(Workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        if (RootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 500 then
                            repeat wait()
                                Humanoid:MoveTo(v.HumanoidRootPart.Position)
                                v.HumanoidRootPart.CanCollide = false
                                RootPart.CFrame = v.HumanoidRootPart.CFrame
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AttackButton")
                            until not Settings.AutoFarm or v.Humanoid.Health <= 0
                        end
                    end
                end
            end)
            wait()
        end
    end)
end

function AutoNewWorld()
    spawn(function()
        while Settings.AutoNewWorld do
            if game.Players.LocalPlayer.Data.Level.Value >= 700 then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelDressrosa")
            end
            wait(1)
        end
    end)
end

function AutoThirdSea()
    spawn(function()
        while Settings.AutoThirdSea do
            if game.Players.LocalPlayer.Data.Level.Value >= 1500 then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelZou")
            end
            wait(1)
        end
    end)
end

function AutoRaid()
    spawn(function()
        while Settings.AutoRaid do
            if game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Timer.Visible == false then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("RaidsNpc", "Select", "Bone")
            end
            wait(1)
        end
    end)
end

function AutoSaber()
    spawn(function()
        while Settings.AutoSaber do
            if game:GetService("Workspace").Enemies:FindFirstChild("Saber Expert") then
                for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    if v.Name == "Saber Expert" then
                        repeat wait()
                            Humanoid:MoveTo(v.HumanoidRootPart.Position)
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AttackButton")
                        until not Settings.AutoSaber or v.Humanoid.Health <= 0
                    end
                end
            end
            wait()
        end
    end)
end

function TeleportToPlayer()
    spawn(function()
        while Settings.TeleportToPlayer do
            if Settings.TargetPlayer ~= "" then
                local target = FindPlayer(Settings.TargetPlayer)
                if target and target.Character then
                    RootPart.CFrame = target.Character.HumanoidRootPart.CFrame
                end
            end
            wait(1)
        end
    end)
end

function FindPlayer(name)
    for i,v in pairs(game.Players:GetPlayers()) do
        if v.Name:lower():find(name:lower()) then
            return v
        end
    end
end

-- Init
UpdateContent()

-- Drag GUI
local dragging
local dragInput
local dragStart
local startPos

local function update(input)
    local delta = input.Position - dragStart
    MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

MainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

MainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)

