local function toggle(p,text,func)
	local s=false
	local b = Instance.new("TextButton", p)
	b.Size = UDim2.new(0.9,0,0,36)
	b.Text = text.." : OFF"
	b.Font = Enum.Font.Gotham
	b.TextSize = 14
	b.TextColor3 = Color3.new(1,1,1)
	b.BackgroundColor3 = Color3.fromRGB(60,60,60)
	Instance.new("UICorner", b).CornerRadius = UDim.new(0,8)
	b.MouseButton1Click:Connect(function()
		s = not s
		b.Text = text.." : "..(s and "ON" or "OFF")
		func(s)
	end)
end

-- ================= AUTO FARM =================
local function getEnemy()
	local near,dist=nil,math.huge
	for _,e in pairs(CollectionService:GetTagged("Enemy")) do
		if e:FindFirstChild("Humanoid") and e.Humanoid.Health>0 then
			local d=(hrp.Position-e.HumanoidRootPart.Position).Magnitude
			if d<dist then dist=d near=e end
		end
	end
	return near
end

RunService.Heartbeat:Connect(function()
	if _G.AutoFarm then
		local e = getEnemy()
		if e then
			hrp.CFrame = e.HumanoidRootPart.CFrame * CFrame.new(0,0,5)
			e.Humanoid:TakeDamage(10)
		end
	end
end)

toggle(pages.Farm,"Auto Farm",function(v) _G.AutoFarm=v end)

